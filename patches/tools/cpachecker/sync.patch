+
+ldv_list_is_stop.parameters = 2
+ldv_list_is_stop.pInfo = READ:1, READ:1
+
+ldv_list_get_next.parameters = 1
+ldv_list_get_next.pInfo = READ:1
+ldv_list_get_next.linkInfo = 0, 1
